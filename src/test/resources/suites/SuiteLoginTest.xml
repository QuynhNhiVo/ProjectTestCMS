<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Page Object test example" verbose="1">

    <test name="Login Test Chrome">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="CMS.testcases.LoginTest">
                <methods>
                    <include name="loginSuccess" />
                </methods>
            </class>
        </classes>
    </test>

    <test name="Login Test Edge">
        <parameter name="browser" value="edge" />
        <classes>
            <class name="CMS.testcases.LoginTest">
                <methods>
                    <include name="loginSuccess" />
                </methods>
            </class>
        </classes>
    </test>
</suite>